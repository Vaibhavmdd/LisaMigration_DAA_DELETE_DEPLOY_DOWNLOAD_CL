<?xml version="1.0" encoding="UTF-8"?>

<amxdata_base:Enterprise
	xmlns:amxdata="http://tibco.com/amxadministrator/command/line/types"
	xmlns:amxdata_jmsssr="http://tibco.com/amxadministrator/command/line/types_jmssr"
	xmlns:amxdata_base="http://tibco.com/amxadministrator/command/line/types_base"
	xmlns:amxdata_reference="http://tibco.com/amxadministrator/command/line/types_reference"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://tibco.com/amxadministrator/command/line/types_base ../schemas/amxdata_base.xsd http://tibco.com/amxadministrator/command/line/types ../schemas/amxdata.xsd">

	<Environment 
		xsi:type="amxdata:Environment" 
		name="[[admin.env.name]]">
		<Application xsi:type="amxdata:Application" 
			name="[[logserviceAppName]]" 
			contact="TIBCO Software Inc." 
			description="Log Service">
			<Property xsi:type="amxdata:Property" 
				name="[[logservicePropertyName]]" 
				value="true"/>
		</Application>
	</Environment>	
	
	<Environment 
		xsi:type="amxdata:Environment" 
		description="QA Environment" 
		name="[[env.name]]">	
		<Node xsi:type="amxdata:Node" name="[[env.node1.name]]" hostName="[[host.name]]">
		    <ResourceInstance xsi:type="amxdata:ResourceInstance" name="httpConnectorForCL0324" resourceTemplateName="httpConnectorForCL0324"/>
		</Node>
				
		<Application
			xsi:type="amxdata:Application"
			name="cl03_24"
			contact="Gavin Chen">
		  <Node name="[[env.node1.name]]" environmentName="[[env.name]]"/>                  	    
			<ApplicationTemplate xsi:type="amxdata_reference:ApplicationTemplate_reference" name="CL-2188"/>
			<!--Configure Logging-->	
			<Logger xsi:type="amxdata:Logger" name ="com.tibco.amx.cl2188" additivity="true">
				<AppenderRef xsi:type="amxdata:AppenderRef" effectiveLevel="INFO">
					<Appender xsi:type="amxdata_reference:LogAppender_reference" name="FileAppender0324"/>
				</AppenderRef>	
				<AppenderRef xsi:type="amxdata:AppenderRef" effectiveLevel="INFO">
					<Appender xsi:type="amxdata_reference:LogAppender_reference" name="JMSAppender0324"/>
				</AppenderRef>		
			</Logger>			
		</Application>	
			
	</Environment>	
	
	<ResourceTemplate 
	  xsi:type="amxdata:HttpConnectorResourceTemplate" 
	  name="httpConnectorForCL0324" 
	  host="[[host1]]" 
	  port="[[servicePort03_24]]" 
		headerBufferSize="512" 
		requestBufferSize="512"	
		responseBufferSize="512" 
		maxIdleTime="200000" 
		lowResourceMaxIdleTime="512" 
		description="This is HttpConnector RT for CL 3.24"/>	
		
	<DAA
		xsi:type="amxdata:DAA"
		location="SoaProject/Deployment Artifacts/CL-2188.daa"
	  importResourceTemplates="false">
	</DAA>
	
	<!--Define Log Appender-->
	<LogAppender
		xsi:type="amxdata:FileLogAppender"
		name="FileAppender0324"
		description="QA File LogAppender0324"
		filePath="[[tibco.test.case.log.dir]]/App0324.log"
		maxSize="10240"
		maxBackupNum="5"/>			
	
	<LogAppender
    xsi:type="amxdata:JmsLogAppender"
    name="JMSAppender0324"
    description="QA JMS LogAppender0324"
    jmsConnectionFactoryName="JmsConnectionFactory0324"
    jmsConnectionName="JndiConnectionConfig0324"
    jmsDestination="JmsDestination0324"
    expireTimeInDB="10"
		payload = "false"	/>
      
  <amxdata_jmsssr:JNDIConnectionConfigurationResourceTemplate 
 		initialContextFactory="com.tibco.tibjms.naming.TibjmsInitialContextFactory" 
  	name="JndiConnectionConfig0324" 
  	xsi:type="amxdata_jmsssr:JNDIConnectionConfigurationResourceTemplate" providerUrl="tibjmsnaming://[[emshost]]:[[emsport]]">
  	<InlineCredentials username="[[emsuser]]" password="[[emspwd]]"/>
  </amxdata_jmsssr:JNDIConnectionConfigurationResourceTemplate>
    
  <amxdata_jmsssr:JNDIConnectionFactoryResourceTemplate 
  	jndiConnectionConfigurationName="JndiConnectionConfig0324" 
  	jndiName="QueueConnectionFactory" 
  	name="JmsConnectionFactory0324" 
  	xsi:type="amxdata_jmsssr:JNDIConnectionFactoryResourceTemplate">
  	<InlineCredentials username="[[emsuser]]" password="[[emspwd]]"/>
  </amxdata_jmsssr:JNDIConnectionFactoryResourceTemplate>
  
  <amxdata_jmsssr:JNDIDestinationResourceTemplate  
  	jndiConnectionConfigurationName="JndiConnectionConfig0324" 
  	jndiName="[[commonlogging.logservice.emsqueue]]" 
  	name="JmsDestination0324" 
  	xsi:type="amxdata_jmsssr:JNDIDestinationResourceTemplate"/>

</amxdata_base:Enterprise>