<?xml version="1.0" encoding="UTF-8"?>

<amxdata_base:Enterprise
	xmlns:amxdata="http://tibco.com/amxadministrator/command/line/types"
	xmlns:amxdata_jmsssr="http://tibco.com/amxadministrator/command/line/types_jmssr"
	xmlns:amxdata_base="http://tibco.com/amxadministrator/command/line/types_base"
	xmlns:amxdata_reference="http://tibco.com/amxadministrator/command/line/types_reference"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://tibco.com/amxadministrator/command/line/types_base ../schemas/amxdata_base.xsd http://tibco.com/amxadministrator/command/line/types ../schemas/amxdata.xsd">

	<Environment 
		xsi:type="amxdata:Environment" 
		description="QA Environment" 
		name="[[env.name]]">	
		<Node xsi:type="amxdata:Node" name="[[env.node1.name]]" hostName="[[host.name]]">
		    <ResourceInstance xsi:type="amxdata:ResourceInstance" name="httpConnectorCL1_13" resourceTemplateName="httpConnectorCL1_13"/>
		</Node>
		
		<Application
			xsi:type="amxdata:Application"
			name="cl1_13"
			contact="LeveonZhao"
			description="cl1_13 application">
                        <Component xsi:type="amxdata:Component" name="JavaComponent">
		             <Node name="[[env.node1.name]]" environmentName="[[env.name]]"/>	
                        </Component>      	
			<PromotedService xsi:type="amxdata_base:Service_base" name="LotteryPortService">
				<Binding xsi:type="amxdata:Binding" name="SOAPService_Binding">
					<Node name="[[env.node1.name]]" environmentName="[[env.name]]" />				
				</Binding>
			</PromotedService>	    
			<ApplicationTemplate xsi:type="amxdata_reference:ApplicationTemplate_reference" name="cl1_13"/>
			
			<!--Configure Logging-->
			

			
			<Logger xsi:type="amxdata:Logger" name ="com.tibco" additivity="true">
				<AppenderRef xsi:type="amxdata:AppenderRef" effectiveLevel="INFO">
					<Appender xsi:type="amxdata_reference:LogAppender_reference" name="qaCBEFileLogAppender0113"/>
				</AppenderRef>			
			</Logger>
			
						
		</Application>		
	</Environment>	
	
	<ResourceTemplate xsi:type="amxdata:HttpConnectorResourceTemplate" name="httpConnectorCL1_13" host="[[host1]]" port="[[servicePort1_13]]" headerBufferSize="512" requestBufferSize="512"	responseBufferSize="512" maxIdleTime="200000" lowResourceMaxIdleTime="512" description="This is HttpConnector RT"/> 
	
	<DAA
		xsi:type="amxdata:DAA"
		description="DAA For cl0113"
		location="SoaProject/Deployment Artifacts/cl1_13.daa"
	        importResourceTemplates="false">
	</DAA>
	
	<!--Define Log Appender-->
		
	
	
		<LogAppender
		xsi:type="amxdata:FileLogAppender"
		name="qaCBEFileLogAppender0113"
		type="cbe"
		description="QA CBE File LogAppender 0113"
		filePath="[[tibco.test.case.log.dir]]/CL1_13_DEBUG.log"
		maxSize="1024"
		maxBackupNum="2"/>		

</amxdata_base:Enterprise>