<?xml version="1.0" encoding="UTF-8"?>

<amxdata_base:Enterprise
	xmlns:amxdata="http://tibco.com/amxadministrator/command/line/types"
	xmlns:amxdata_jmsssr="http://tibco.com/amxadministrator/command/line/types_jmssr"
	xmlns:amxdata_base="http://tibco.com/amxadministrator/command/line/types_base"
	xmlns:amxdata_reference="http://tibco.com/amxadministrator/command/line/types_reference"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://tibco.com/amxadministrator/command/line/types_base ../schemas/amxdata_base.xsd http://tibco.com/amxadministrator/command/line/types ../schemas/amxdata.xsd">

	<Environment 
		xsi:type="amxdata:Environment" 
		description="QA Environment" 
		name="[[env.name]]">	
		<Node xsi:type="amxdata:Node" name="[[env.node1.name]]" hostName="[[host.name]]">
		    <ResourceInstance xsi:type="amxdata:ResourceInstance" name="httpConnectorCL3_11" resourceTemplateName="httpConnectorCL3_11"/>
		</Node>
		
		<Application
			xsi:type="amxdata:Application"
			name="cl3_11"
			contact="LeveonZhao"
			description="cl3_11 application">
      <Component xsi:type="amxdata:Component" name="PayloadJMSAppender">
		  	<Node name="[[env.node1.name]]" environmentName="[[env.name]]"/>	
      </Component>      
      <!--SVar xsi:type="amxdata_base:SubstitutionVariable" name="JNDIConnectionFactory" type="String" value="cl_QA_logservice_jmsConnectionFactory" description="JMS Appender svar" />
			<SVar xsi:type="amxdata_base:SubstitutionVariable" name="JNDIConnection" type="String" value="cl_QA_logservice_jndiConnectionConfig" description="JMS Appender svar" />
			<SVar xsi:type="amxdata_base:SubstitutionVariable" name="JNDIDestination" type="String" value="cl_QA_logservice_jmsDestination" description="JMS Appender svar" /-->
			<SVar xsi:type="amxdata_base:SubstitutionVariable" name="LogMessageExpirationTime" type="Integer" value="8" description="JMS Appender svar" />
			<!--SVar xsi:type="amxdata_base:SubstitutionVariable" name="PayloadConnectionFactory" type="String" value="cl_QA_payloadservice_jmsConnectionFactory" description="JMS Appender svar" />
			<SVar xsi:type="amxdata_base:SubstitutionVariable" name="PayloadConnection" type="String" value="cl_QA_payloadservice_jndiConnectionConfig" description="JMS Appender svar" />
			<SVar xsi:type="amxdata_base:SubstitutionVariable" name="PayloadDestination" type="String" value="cl_QA_payloadservice_jmsDestination" description="JMS Appender svar" /-->
			<SVar xsi:type="amxdata_base:SubstitutionVariable" name="SharedDiskURL" type="String" value="C:\temp" description="JMS Appender svar" />
			<PromotedService xsi:type="amxdata_base:Service_base" name="SoapComp">
				<Binding xsi:type="amxdata:Binding" name="SoapComp">
					<Node name="[[env.node1.name]]" environmentName="[[env.name]]" />				
				</Binding>
			</PromotedService>	    
			
			
			
			<ApplicationTemplate xsi:type="amxdata_reference:ApplicationTemplate_reference" name="cl3_11"/>
			
			<!--Configure Logging-->
			
			<Logger xsi:type="amxdata:Logger" name ="com.tibco" additivity="false">
			
				<AppenderRef xsi:type="amxdata:AppenderRef" effectiveLevel="DEBUG">
					<Appender xsi:type="amxdata_reference:LogAppender_reference" name="QA_JMSAppender311"/>
				</AppenderRef>			
			
			
			</Logger>		
		</Application>		
	</Environment>	
	
	<ResourceTemplate xsi:type="amxdata:HttpConnectorResourceTemplate" name="httpConnectorCL3_11" host="[[host1]]" port="[[servicePort3_11]]" headerBufferSize="512" requestBufferSize="512"	responseBufferSize="512" maxIdleTime="200000" lowResourceMaxIdleTime="512" description="This is HttpConnector RT"/> 
	
	<DAA
		xsi:type="amxdata:DAA"
		description="DAA For cl0311"
		location="JmsAppenderForPayload/Deployment Artifacts/cl3_11.daa"
	        importResourceTemplates="false">
	</DAA>
	
	<!--Define Log Appender-->
		
	
	 <LogAppender
      xsi:type="amxdata:JmsLogAppender"
      name="QA_JMSAppender311"
      description="[[JMSappenderDescription]]"
      jmsConnectionFactoryName="cl_QA_logservice_jmsConnectionFactory"
      jmsConnectionName="cl_QA_logservice_jndiConnectionConfig"
      jmsDestination="cl_QA_logservice_jmsDestination"
      expireTimeInDB="%%LogMessageExpirationTime%%"
			payload = "true"		
			payloadConnectionFactoryName ="cl_QA_payloadservice_jmsConnectionFactory" 
			payloadConnectionName="cl_QA_payloadservice_jndiConnectionConfig"
			payloadDestination="cl_QA_payloadservice_jmsDestination"
			sharedDiskURL="%%SharedDiskURL%%"/>
  	
         
         
         <!--
      		type="jndi"
		sync="false"
		sharedDiskURL="C:\temp"
		-->
      
  <amxdata_jmsssr:JNDIConnectionConfigurationResourceTemplate 
  description="[[LogserviceRT_JndiConnectionConfiguration]]" 
  initialContextFactory="com.tibco.tibjms.naming.TibjmsInitialContextFactory" 
  name="cl_QA_logservice_jndiConnectionConfig" 
  xsi:type="amxdata_jmsssr:JNDIConnectionConfigurationResourceTemplate" providerUrl="tibjmsnaming://[[emshost]]:[[emsport]]">
  <InlineCredentials username="[[emsuser]]" password="[[emspwd]]"/>
  </amxdata_jmsssr:JNDIConnectionConfigurationResourceTemplate>
    
  <amxdata_jmsssr:JNDIConnectionFactoryResourceTemplate 
  description="[[LogserviceRT_JndiConnectionFactory]]" 
  jndiConnectionConfigurationName="cl_QA_logservice_jndiConnectionConfig" 
  jndiName="GenericConnectionFactory" 
  name="cl_QA_logservice_jmsConnectionFactory" 
  xsi:type="amxdata_jmsssr:JNDIConnectionFactoryResourceTemplate">
  <InlineCredentials username="[[emsuser]]" password="[[emspwd]]"/>
  </amxdata_jmsssr:JNDIConnectionFactoryResourceTemplate>
  
  
  <amxdata_jmsssr:JNDIDestinationResourceTemplate 
  description="[[LogserviceRT_JndiDestination]]" 
  jndiConnectionConfigurationName="cl_QA_logservice_jndiConnectionConfig" 
  jndiName="cl_logservice_queue" 
  name="cl_QA_logservice_jmsDestination" 
  xsi:type="amxdata_jmsssr:JNDIDestinationResourceTemplate"/>


     	<!--RT for payload part configuration of JMS appender/-->
     	
     
 <amxdata_jmsssr:JNDIConnectionConfigurationResourceTemplate description="[[PayloadserviceRT_JndiConnectionConfiguration]]" 
     initialContextFactory="com.tibco.tibjms.naming.TibjmsInitialContextFactory" 
     name="cl_QA_payloadservice_jndiConnectionConfig" 
     xsi:type="amxdata_jmsssr:JNDIConnectionConfigurationResourceTemplate" providerUrl="tibjmsnaming://[[emshost]]:[[emsport]]">
    <InlineCredentials username="[[emsuser]]" password="[[emspwd]]"/>
  </amxdata_jmsssr:JNDIConnectionConfigurationResourceTemplate>
  
  <amxdata_jmsssr:JNDIConnectionFactoryResourceTemplate description="[[PayloadserviceRT_JndiConnectionFactory]]" 
    jndiConnectionConfigurationName="cl_QA_payloadservice_jndiConnectionConfig" 
    jndiName="GenericConnectionFactory" 
    name="cl_QA_payloadservice_jmsConnectionFactory" 
    xsi:type="amxdata_jmsssr:JNDIConnectionFactoryResourceTemplate">
    <InlineCredentials username="[[emsuser]]" password="[[emspwd]]"/>
  </amxdata_jmsssr:JNDIConnectionFactoryResourceTemplate>
  
  
  <amxdata_jmsssr:JNDIDestinationResourceTemplate 
  description="[[PayloadserviceRT_JndiDestination]]" 
  jndiConnectionConfigurationName="cl_QA_payloadservice_jndiConnectionConfig" 
  jndiName="cl_payload_queue" 
  name="cl_QA_payloadservice_jmsDestination" 
  xsi:type="amxdata_jmsssr:JNDIDestinationResourceTemplate"/>	
		
		
			

</amxdata_base:Enterprise>