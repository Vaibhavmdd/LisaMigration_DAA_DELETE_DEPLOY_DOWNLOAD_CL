<project default="deploy.myapp">
	<import file="${TIBCO_AMX_LIBRARY}/bin/amx-macrodefs.xml" />
<target name="setup.automationenvironment1">
<create.environment envName="NewDevEnvironment" dataFile="${amxConfigFile}" />
<create.node envName="NewDevEnvironment" dataFile="${amxConfigFile}" />
<install.node envName="NewDevEnvironment" dataFile="${amxConfigFile}"/>
<start.node envName="NewDevEnvironment" dataFile="${amxConfigFile}"/>
<create.rt dataFile="${amxConfigFile}"/>
<create.ri dataFile="${amxConfigFile}"/>
<install.ri dataFile="${amxConfigFile}"/>
</target>
	<target name="deploy.myapp">
		<upload.daa dataFile="${amxConfigFile}" />
		<create.app appName="AMXTestCase1.10" dataFile="${amxConfigFile}" />
		<map.component.to.node appName="AMXTestCase1.10" dataFile="${amxConfigFile}"/>
		<map.service.binding.to.node appName="AMXTestCase1.10" dataFile="${amxConfigFile}" />
		<set.service.binding.property appName="AMXTestCase1.10" dataFile="${amxConfigFile}" />
		<deploy.app appName="AMXTestCase1.10" dataFile="${amxConfigFile}"/>
		<promote.service appName="AMXTestCase1.10" dataFile="${amxConfigFile}"/>
		
</target>


	
	<target name="deploy.mynewapp">


		<upload.daa dataFile="${amxConfigFile}" />
		<create.app appName="AMXTestCase1.10.1" dataFile="${amxConfigFile}" />
		<map.component.to.node appName="AMXTestCase1.10.1" dataFile="${amxConfigFile}"/>
		<map.service.binding.to.node appName="AMXTestCase1.10.1" dataFile="${amxConfigFile}" />
		<set.service.binding.property appName="AMXTestCase1.10.1" dataFile="${amxConfigFile}" />
		<map.reference.binding.to.node appName="AMXTestCase1.10.1" dataFile="${amxConfigFile}"/>
		<set.reference.binding.property dataFile="${amxConfigFile}"/>
		<promote.reference  appName="AMXTestCase1.10.1"  dataFile="${amxConfigFile}"/>
		<set.reference.wiring.across.env referenceName="AMXNewPromotedReference" dataFile="${amxConfigFile}" />
		<deploy.app appName="AMXTestCase1.10.1" dataFile="${amxConfigFile}"/>


              
		
</target>

	<target name="undeploy.app">
		<undeploy.app dataFile="${amxConfigFile}"  />
	</target>


		
</project>

