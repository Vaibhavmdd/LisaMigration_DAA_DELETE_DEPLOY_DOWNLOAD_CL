<project default="deploy.myapp">
	<import file="${TIBCO_AMX_LIBRARY}/bin/amx-macrodefs.xml" />

	<target name="deploy.myapp">
		<upload.daa dataFile="${amxConfigFile}" />
		<create.app dataFile="${amxConfigFile}" />
		<create.app dataFile="${amxConfigFile}" />
		<map.component.to.node dataFile="${amxConfigFile}"/>
		<map.service.binding.to.node dataFile="${amxConfigFile}" />
		<set.service.binding.property dataFile="${amxConfigFile}" />
		<map.reference.binding.to.node dataFile="${amxConfigFile}"/>
		<set.reference.binding.property dataFile="${amxConfigFile}"/>
		<deploy.app appName="AMXTestCase1.5" dataFile="${amxConfigFile}"/>
		<deploy.app appName="AMXTestCase1.5.1" dataFile="${amxConfigFile}"/>
	</target>
	<target name="undeploy.myapp">
		<undeploy.app appName="AMXTestCase1.5.1" dataFile="${amxConfigFile}"  />
	</target>
	<target name="deploy.mynewapp">
		<deploy.app appName="AMXTestCase1.5.2" dataFile="${amxConfigFile}"/>
		<map.reference.binding.to.node dataFile="${amxConfigFile}"/>
		<set.reference.binding.property dataFile="${amxConfigFile}"/>
		<deploy.app appName="AMXTestCase1.5.1" dataFile="${amxConfigFile}"/>
		
	</target>

	<target name="undeploy.app">
		<undeploy.app dataFile="${amxConfigFile}"  />
	</target>

		
</project>

