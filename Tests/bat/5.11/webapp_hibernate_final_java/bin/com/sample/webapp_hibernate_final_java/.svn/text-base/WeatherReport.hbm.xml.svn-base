<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-lazy="false">
    <class name="com.sample.webapp_hibernate_final_java.WeatherReport">
        <id name="id">
            <generator class="native"/>
        </id>
        <property name="city" not-null="true"/>
        <property name="state" not-null="true"/>
        <property name="zip" not-null="true"/>
        <property name="forecast" not-null="true"/>
        <property name="high" not-null="true"/>
        <property name="low" not-null="true"/>
    </class>
</hibernate-mapping>

