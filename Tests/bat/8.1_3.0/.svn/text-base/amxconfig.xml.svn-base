<?xml version="1.0" encoding="UTF-8"?>

<amxdata_base:Enterprise
	xmlns:amxdata="http://tibco.com/amxadministrator/command/line/types"
	xmlns:amxdata_base="http://tibco.com/amxadministrator/command/line/types_base"
	xmlns:amxdata_reference="http://tibco.com/amxadministrator/command/line/types_reference"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://tibco.com/amxadministrator/command/line/types_base ../schemas/amxdata_base.xsd http://tibco.com/amxadministrator/command/line/types ../schemas/amxdata.xsd">

	<Environment 
		xsi:type="amxdata:Environment" 
	    name="[[env.name]]" 
	    description="QAEnvironment_Administrator environment" 
	    contact="TIBCO">
		<Node xsi:type="amxdata:Node" name="[[env.node1.name]]" hostName="[[host.name]]" description="My first node" contact="TIBCO" >
			<ResourceInstance xsi:type="amxdata:ResourceInstance" name="HttpClient_SOAPEventSource" resourceTemplateName="HttpClient_BAT8.1_HttpClient_SOAPEventSource"/>
			<ResourceInstance xsi:type="amxdata:ResourceInstance" name="HttpConnector_BAT8.1_Invoke" resourceTemplateName="HttpConnector_BAT8.1_Invoke"/>
		</Node>
		
		<Node xsi:type="amxdata:Node" name="[[env.node2.name]]" hostName="[[host.name]]" description="My Second node" contact="TIBCO" >
			<ResourceInstance xsi:type="amxdata:ResourceInstance" name="HttpConnector_BAT8.1_Provider" resourceTemplateName="HttpConnector_BAT8.1_Provider"/>
		</Node>
		<Application 
			xsi:type="amxdata:Application" 
			name="BAT8.1_SOAP2JavaCallJava3Stop_Server"
			contact="Boen Zhang"
			description="SOAP-Java1-Java2 -Reference Call SOAP-Java3 (Stop Java3)">
			
			<Component xsi:type="amxdata_base:Component_base" name="JavaHelloComponent">
				<Node xsi:type="amxdata_reference:Node_reference" name="[[env.node2.name]]" environmentName="[[env.name]]"/>
			</Component>
			<PromotedService xsi:type="amxdata_base:Service_base" name="HelloWorldPT">
				<Binding xsi:type="amxdata_base:Binding_base" name="SOAPService_Binding1">
					<Node xsi:type="amxdata_reference:Node_reference" name="[[env.node2.name]]" environmentName="[[env.name]]"/>
					<Property xsi:type="amxdata:Property" name="HttpInboundConnectionConfig" value="HttpConnector_BAT8.1_Provider"/>
				</Binding>
			</PromotedService>
			<ApplicationTemplate xsi:type="amxdata_reference:ApplicationTemplate_reference" name="server"/>
		</Application>
			
		<Application 
			xsi:type="amxdata:Application" 
			name="BAT8.1_SOAP2JavaCallJava3Stop_Client"
			contact="Boen Zhang"
			description="SOAP-Java1-Java2 -Reference Call SOAP-Java3 (Stop Java3)">
			
			<Component xsi:type="amxdata_base:Component_base" name="Java1">
				<Node xsi:type="amxdata_reference:Node_reference" name="[[env.node1.name]]" environmentName="[[env.name]]"/>
			</Component>
			<Component xsi:type="amxdata_base:Component_base" name="Java2">
				<Node xsi:type="amxdata_reference:Node_reference" name="[[env.node1.name]]" environmentName="[[env.name]]"/>
			</Component>
			<PromotedService xsi:type="amxdata_base:Service_base" name="HelloWorldPT">
				<Binding xsi:type="amxdata_base:Binding_base" name="SOAPService_Binding1">
					<Node xsi:type="amxdata_reference:Node_reference" name="[[env.node1.name]]" environmentName="[[env.name]]"/>
					<Property xsi:type="amxdata:Property" name="HttpInboundConnectionConfig" value="HttpConnector_BAT8.1_Invoke"/>
				</Binding>
			</PromotedService>
			<PromotedReference xsi:type="amxdata_base:Reference_base" name="HelloWorldPT1">
				<Binding xsi:type="amxdata:Binding" name="SOAPReference_Binding1">
        	<Node xsi:type="amxdata_reference:Node_reference" name="[[env.node1.name]]" environmentName="[[env.name]]"/>
				</Binding>
			</PromotedReference> 
			<ApplicationTemplate xsi:type="amxdata_reference:ApplicationTemplate_reference" name="client"/>
		</Application>	
	</Environment>

	<ResourceTemplate 
		xsi:type="amxdata:HttpConnectorResourceTemplate" 
		name="HttpConnector_BAT8.1_Invoke" 
		host="0.0.0.0" 
		port="[[HttpConnector_BAT8.1_Invoke_port]]" 
		headerBufferSize="512" 
		requestBufferSize="512" 
		responseBufferSize="512" 
		maxIdleTime="200000" 
		lowResourceMaxIdleTime="512" 
		description="This is HttpConnector RT for SOAP HTTP"/>
		
	<ResourceTemplate 
		xsi:type="amxdata:HttpConnectorResourceTemplate" 
		name="HttpConnector_BAT8.1_Provider" 
		host="0.0.0.0" 
		port="[[HttpConnector_BAT8.1_Provider_port]]" 
		headerBufferSize="512" 
		requestBufferSize="512" 
		responseBufferSize="512" 
		maxIdleTime="200000" 
		lowResourceMaxIdleTime="512" 
		description="This is HttpConnector RT for SOAP HTTP"/>
	<ResourceTemplate 
		xsi:type="amxdata:HttpClientResourceTemplate" 	
		name="HttpClient_BAT8.1_HttpClient_SOAPEventSource" 
		isAcceptRedirect="true" 	
		host="0.0.0.0" 
		port="[[HttpConnector_BAT8.1_Provider_port]]" 
		connectionTimeout="8888" 
		soTimeout="20000" 
		description="This is HttpClient RT" />
	
	<DAA
		xsi:type="amxdata:DAA"
		description="DAA for BAT BAT8.1"
		location="SOA/jv.helloworld1.soa/Deployment Artifacts/server.daa"
	    importResourceTemplates="false">
	</DAA>
	<DAA
		xsi:type="amxdata:DAA"
		description="DAA for BAT BAT8.1"
		location="SOA/jv.helloworld1.soa/Deployment Artifacts/client.daa"
	    importResourceTemplates="false">
	</DAA>
	
</amxdata_base:Enterprise>
