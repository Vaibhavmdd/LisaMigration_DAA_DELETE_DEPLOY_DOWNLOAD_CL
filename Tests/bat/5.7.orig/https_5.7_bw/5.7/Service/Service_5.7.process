<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pfx="http://www.example.com/HelloWorld1" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:ns="http://ns.tibco.com/Hello1/" xmlns:ns1="http://www.tibco.com/pe/DeployedVarsType">
    <wsdl:import namespace="http://www.example.com/HelloWorld1" location="/5.7/Service/HelloWorld.wsdl"/>
    <pd:name>5.7/Service/Service_5.7.process</pd:name>
    <pd:startName>SOAPEventSource</pd:startName>
    <pd:startX>0</pd:startX>
    <pd:startY>0</pd:startY>
    <pd:returnBindings/>
    <pd:starter name="SOAPEventSource">
        <pd:type>com.tibco.plugin.soap.SOAPEventSource</pd:type>
        <pd:resourceType>ae.activities.SOAPEventSourceUI</pd:resourceType>
        <pd:x>220</pd:x>
        <pd:y>103</pd:y>
        <config>
            <service>pfx:HelloWorld1PT</service>
            <wsdlNamespaceRoot>http://www.example.com/HelloWorld1/sayHelloWorld1Impl</wsdlNamespaceRoot>
            <operation>sayHelloWorld</operation>
            <portAddressPrefix/>
            <operationStyle>rpc</operationStyle>
            <soapUse>literal</soapUse>
            <embedWsdlComponents>true</embedWsdlComponents>
            <embedSchemaComponents>true</embedSchemaComponents>
            <soapAction>/5.7/Service/Service_5.7</soapAction>
            <inputAttachmentBindings/>
            <outputAttachmentBindings/>
            <sharedChannel>/5.7/SharedResource/HTTS_6767.sharedhttp</sharedChannel>
            <inputNamespace>http://InputMessageNamespace</inputNamespace>
            <outputNamespace>http://OutputMessageNamespace</outputNamespace>
            <faultNamespace>http://FaultMessageNamespace</faultNamespace>
        </config>
        <pd:inputBindings/>
    </pd:starter>
    <pd:endName>End</pd:endName>
    <pd:endX>735</pd:endX>
    <pd:endY>103</pd:endY>
    <pd:errorSchemas/>
    <pd:processVariables/>
    <pd:targetNamespace>http://xmlns.example.com/1269879094065</pd:targetNamespace>
    <pd:activity name="SOAPSendReply">
        <pd:type>com.tibco.plugin.soap.SOAPSendReplyActivity</pd:type>
        <pd:resourceType>ae.activities.SOAPSendReplyUI</pd:resourceType>
        <pd:x>462</pd:x>
        <pd:y>52</pd:y>
        <config>
            <eventSource>SOAPEventSource</eventSource>
        </config>
        <pd:inputBindings>
            <sayHelloWorldResponse>
                <greeting>
                    <ns:HelloWorldResponse>
                        <xsl:value-of select="concat(&quot;Hello &quot;, $SOAPEventSource/sayHelloWorld/name/ns:HelloWorldRequest)"/>
                    </ns:HelloWorldResponse>
                </greeting>
            </sayHelloWorldResponse>
        </pd:inputBindings>
    </pd:activity>
    <pd:activity name="SOAPSendFault">
        <pd:type>com.tibco.plugin.soap.SOAPSendFaultActivity</pd:type>
        <pd:resourceType>ae.activities.SOAPSendFaultUI</pd:resourceType>
        <pd:x>456</pd:x>
        <pd:y>177</pd:y>
        <config>
            <faultMessage>&lt;Default&gt;</faultMessage>
            <eventSource>SOAPEventSource</eventSource>
        </config>
        <pd:inputBindings>
            <defaultFaultElement>
                <faultcode>
                    <xsl:value-of select="&quot;Fault from BW&quot;"/>
                </faultcode>
                <faultstring>
                    <xsl:value-of select="&quot;Fault from BW&quot;"/>
                </faultstring>
            </defaultFaultElement>
        </pd:inputBindings>
    </pd:activity>
    <pd:transition>
        <pd:from>SOAPEventSource</pd:from>
        <pd:to>SOAPSendReply</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SOAPSendReply</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SOAPEventSource</pd:from>
        <pd:to>SOAPSendFault</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>error</pd:conditionType>
    </pd:transition>
    <pd:transition>
        <pd:from>SOAPSendFault</pd:from>
        <pd:to>End</pd:to>
        <pd:lineType>Default</pd:lineType>
        <pd:lineColor>-16777216</pd:lineColor>
        <pd:conditionType>always</pd:conditionType>
    </pd:transition>
</pd:ProcessDefinition>