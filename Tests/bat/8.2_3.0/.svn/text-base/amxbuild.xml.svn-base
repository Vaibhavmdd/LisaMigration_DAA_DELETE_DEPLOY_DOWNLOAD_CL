<project name="host_build" default="e2e">
        <import file="${TIBCO_AMXADMIN_HOME}/bin/taskdef.xml" />
      
        <import file="${TIBCO_AMX_LIBRARY}/bin/amx-macrodefs.xml" />
          
	<target name="create.host">
		<AMXAdminTask
			remote="true"
			propsFile="${remotePropsFile}"
			action="add"
			dataFile="${amxConfigFile}"
			objectSelector="Host"
			overwrite="true"
			merge="true"
			createIfNotExists="true"
			failOnError="true"/>
	</target>
	
	
	<target name="install.host">
		 <AMXAdminTask
			remote="true"
			propsFile="${remotePropsFile}"
			action="install"
			dataFile="${amxConfigFile}"
			objectSelector="Host"
			failOnError="true"/>
		  </target>
			
	  <target name="start.host">
		 <AMXAdminTask
			remote="true"
			propsFile="${remotePropsFile}"
			action="start"
			dataFile="${amxConfigFile}"
			objectSelector="Host"
			failOnError="true"/>
	  </target>
	  
	 
	  
	  <target name="create.node">
		<AMXAdminTask
			action="add"
			objectSelector="Environment/Node"
			remote="true"
			propsFile="${remotePropsFile}"
			dataFile="${amxConfigFile}"
			overwrite="true"
			merge="true"
			createIfNotExists="true"
			force="true"
			failOnError="true"/>
	</target>
	
	<target name="install.node">
		<AMXAdminTask
			action="install"
			objectSelector="Environment/Node"
			remote="true"
			propsFile="${remotePropsFile}"
			dataFile="${amxConfigFile}"
			overwrite="true"
			merge="true"
			createIfNotExists="true"
			force="true"
			failOnError="true"/>
	</target>
	
	<target name="start.node">
		<AMXAdminTask
			action="start"
			objectSelector="Environment/Node"
			remote="true"
			propsFile="${remotePropsFile}"
			dataFile="${amxConfigFile}"
			failOnError="true"/>
	</target>
	<target name="stop.node">
		<AMXAdminTask
			action="stop"
			objectSelector="Environment/Node"
			remote="true"
			propsFile="${remotePropsFile}"
			dataFile="${amxConfigFile}"
			failOnError="true"/>
	</target>
	<target name="uninstall.node">
		<AMXAdminTask
			action="uninstall"
			objectSelector="Environment/Node"
			remote="true"
			propsFile="${remotePropsFile}"
			dataFile="${amxConfigFile}"
			failOnError="true"/>
	</target>
	<target name="delete.node">
		<AMXAdminTask
			action="delete"
			objectSelector="Environment/Node"
			remote="true"
			propsFile="${remotePropsFile}"
			dataFile="${amxConfigFile}"
			failOnError="true"/>
	</target>
	<target name="stop.host">
		<AMXAdminTask
			action="stop"
			objectSelector="Host"
			remote="true"
			propsFile="${remotePropsFile}"
			dataFile="${amxConfigFile}"
			failOnError="true"/>
       </target>   
	   <target name="uninstall.host">
		   <AMXAdminTask
			remote="true"
			propsFile="${remotePropsFile}"
			action="uninstall"
			dataFile="${amxConfigFile}"
			force="false"
			objectSelector="Host"
			failOnError="true"/>
	    </target>
	<target name="delete.host">
		<AMXAdminTask
			remote="true"
			propsFile="${remotePropsFile}"
			action="delete"
			dataFile="${amxConfigFile}"
			objectSelector="Host"
			force="false"
			failOnError="true"/>
	</target>
	
</project>
