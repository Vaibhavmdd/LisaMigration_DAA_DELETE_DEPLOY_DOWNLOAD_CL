<?xml version="1.0" encoding="UTF-8"?>
<pd:ProcessDefinition xmlns:ns1="http://www.tibco.com/pe/EngineTypes" xmlns:pfx="http://tibco.com/matrix/qa/wsdl/group" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:ns3="http://tibco.com/matrix/qa/wsdl/group/bwService" xmlns:ns="http://www.tibco.com/pe/GenerateErrorActivity/InputSchema" xmlns:ns2="http://tibco.com/matrix/qa/xsd/group" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:pd="http://xmlns.tibco.com/bw/process/2003" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/">
    <wsdl:import namespace="http://tibco.com/matrix/qa/wsdl/group" location="/BookStore.wsdl"/>
