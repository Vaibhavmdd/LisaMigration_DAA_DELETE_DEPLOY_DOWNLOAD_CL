                </ns2:BookStore>
                <ns2:Echo>
                    <xsl:value-of select="$Start/pfx:GetBooksInput/InPart/ns2:Echo"/>
                </ns2:Echo>
            </OutPart>
        </pfx:GetBooksOutput>
    </pd:returnBindings>
    <pd:endName>End</pd:endName>
    <pd:endType wsMsgRef="pfx:GetBooksOutput"/>
    <pd:endX>543</pd:endX>
    <pd:endY>88</pd:endY>
