        <pd:resourceType>ae.activities.throw</pd:resourceType>
        <pd:x>402</pd:x>
        <pd:y>178</pd:y>
        <config>
            <faultName>BookFault</faultName>
        </config>
        <pd:inputBindings>
            <ns:ActivityInput>
                <message>
                    <xsl:value-of select="&quot;Fault from BW Provider&quot;"/>
                </message>
                <messageCode>
                    <xsl:value-of select="&quot;fault message code&quot;"/>
                </messageCode>
                <data>
                    <pfx:GetBooksFault>
                        <fault>
                            <ns2:BookStoreFault>
                                <xsl:value-of select="concat(&quot;Invalid Author &quot;, $Start/pfx:GetBooksInput/InPart/ns2:BookStore/ns2:Author, &quot; with title - &quot;,$Start/pfx:GetBooksInput/InPart/ns2:BookStore/ns2:Title)"/>
