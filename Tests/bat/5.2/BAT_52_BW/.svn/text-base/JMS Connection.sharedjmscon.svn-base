<?xml version="1.0" encoding="UTF-8"?>
<BWSharedResource>
    <name>JMS Connection</name>
    <resourceType>ae.shared.JMSConnectionKey</resourceType>
    <config>
        <NamingEnvironment>
            <UseJNDI>true</UseJNDI>
            <ProviderURL/>
            <NamingURL>%%bw5.2/jndi.contexturl%%</NamingURL>
            <NamingInitialContextFactory>%%bw5.2/jndi.contextfactory%%</NamingInitialContextFactory>
            <TopicFactoryName>TopicConnectionFactory</TopicFactoryName>
            <QueueFactoryName>QueueConnectionFactory</QueueFactoryName>
            <NamingPrincipal>%%bw5.2/jndi.uname%%</NamingPrincipal>
            <NamingCredential>%%bw5.2/jndi.password%%</NamingCredential>
        </NamingEnvironment>
        <ConnectionAttributes>
            <username>%%bw5.2/jms.uname%%</username>
            <password>%%bw5.2/jms.password%%</password>
            <clientID/>
            <autoGenClientID>true</autoGenClientID>
        </ConnectionAttributes>
        <UseXACF>false</UseXACF>
        <UseSharedJndiConfig>false</UseSharedJndiConfig>
    </config>
</BWSharedResource>