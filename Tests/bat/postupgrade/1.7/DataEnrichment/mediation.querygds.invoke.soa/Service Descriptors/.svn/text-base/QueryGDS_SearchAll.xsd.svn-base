<?xml version="1.0" encoding="UTF-8"?>
<xs:schema targetNamespace="http://com.tibco.amx.it.mediation.sample/QueryGDS/searchAll/schemas" 
	   xmlns:tns="http://com.tibco.amx.it.mediation.sample/QueryGDS/searchAll/schemas"  
	   xmlns:xs="http://www.w3.org/2001/XMLSchema"  
	   attributeFormDefault="unqualified"
	   elementFormDefault="qualified">
            <xs:complexType name="Address">
                <xs:sequence>
                    <xs:element name="street" type="xs:string"/>
                    <xs:element name="city" type="xs:string"/>
                    <xs:element name="state" type="xs:string"/>
                    <xs:element name="zip" type="xs:string"/>
                    <xs:element name="country" type="xs:string"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="AirlineSearch">
                <xs:sequence>
                    <xs:element name="roundTrip" type="xs:boolean"/>
                    <xs:element name="origin" type="tns:AirportCode"/>
                    <xs:element name="destination" type="tns:AirportCode"/>
                    <xs:element name="departureDate" type="xs:date"/>
                    <xs:element name="returnDate" type="xs:date" minOccurs="0"/>
                    <xs:element name="noOfPassengers" type="xs:int"/>
                    <xs:element name="timePreference" type="xs:string"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="AirlineSearchResults">
                <xs:sequence>
                    <xs:element name="departingFlights" type="tns:DepartingFlights"/>
                    <xs:element name="returningFlights" type="tns:ReturningFlights" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>
            <xs:simpleType name="AirportCode">
                <xs:restriction base="xs:string">
                    <xs:length value="3"/>
                    <xs:pattern value="[A-Z]+"/>
                </xs:restriction>
            </xs:simpleType>
            <xs:complexType name="CarOptions">
                <xs:sequence>
                    <xs:element name="car" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="carType" type="xs:string"/>
                                <xs:element name="carMake" type="xs:string" minOccurs="0"/>
                                <xs:element name="navigation" type="xs:boolean" minOccurs="0"/>
                                <xs:element name="otherFeatures" type="xs:string" minOccurs="0"/>
                                <xs:element name="avgCostPerDay" type="xs:string"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="CarSearch">
                <xs:sequence>
                    <xs:element name="country" type="xs:string"/>
                    <xs:element name="state" type="xs:string"/>
                    <xs:element name="pickUpCity" type="xs:string"/>
                    <xs:element name="dropOffCity" type="xs:string" minOccurs="0"/>
                    <xs:element name="pickUpDate" type="xs:dateTime"/>
                    <xs:element name="dropOffDate" type="xs:dateTime"/>
                    <xs:element name="rentalAgency" type="xs:string" minOccurs="0"/>
                    <xs:element name="carType" type="xs:string" minOccurs="0"/>
                    <xs:element name="carMake" type="xs:string" minOccurs="0"/>
                    <xs:element name="nonSmoking" type="xs:boolean" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="CarSearchResults">
                <xs:sequence>
                    <xs:element name="pickUpCity" type="xs:string"/>
                    <xs:element name="pickUpDate" type="xs:dateTime"/>
                    <xs:element name="dropOffDate" type="xs:dateTime"/>
                    <xs:element name="noOfDays" type="xs:int"/>
                    <xs:element name="rentalAgencies" type="tns:RentalAgencies"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="DepartingFlights">
                <xs:sequence>
                    <xs:element name="flightPlan" type="tns:FlightPlan" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="origin" type="xs:string"/>
                    <xs:element name="destination" type="xs:string"/>
                    <xs:element name="date" type="xs:date"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="FlightPlan">
                <xs:sequence>
                    <xs:element name="airline" type="xs:string"/>
                    <xs:element name="flightNumber" type="xs:string"/>
                    <xs:element name="departureTime" type="xs:time"/>
                    <xs:element name="arrivalTime" type="xs:time"/>
                    <xs:element name="stops" type="xs:int"/>
                    <xs:element name="travelTimeMin" type="xs:int"/>
                    <xs:element name="fareFull" type="xs:float"/>
                    <xs:element name="fareRestricted" type="xs:float"/>
                    <xs:element name="fareSpecial" type="xs:float"/>
                    <xs:element name="farePromotion" type="xs:float"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="HotelDetail">
                <xs:sequence>
                    <xs:element name="hotelName" type="xs:string"/>
                    <xs:element name="address" type="tns:Address"/>
                    <xs:element name="starRating" type="xs:string"/>
                    <xs:element name="amenities" type="xs:string"/>
                    <xs:element name="directions" type="xs:string"/>
                    <xs:element name="comments" type="xs:string"/>
                    <xs:element name="roomOptions" type="tns:RoomOptions"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="HotelSearch">
                <xs:sequence>
                    <xs:element name="country" type="xs:string"/>
                    <xs:element name="city" type="xs:string"/>
                    <xs:element name="state" type="xs:string"/>
                    <xs:element name="nearAddress" type="xs:string" minOccurs="0"/>
                    <xs:element name="hotelName" type="xs:string" minOccurs="0"/>
                    <xs:element name="checkInDate" type="xs:date"/>
                    <xs:element name="checkOutDate" type="xs:date"/>
                    <xs:element name="rooms" type="xs:int"/>
                    <xs:element name="nonSmoking" type="xs:boolean" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="HotelSearchResults">
                <xs:sequence>
                    <xs:element name="city" type="xs:string"/>
                    <xs:element name="checkInDate" type="xs:date"/>
                    <xs:element name="checkOutDate" type="xs:date"/>
                    <xs:element name="noOfNights" type="xs:int"/>
                    <xs:element name="hotels" type="tns:Hotels"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="Hotels">
                <xs:sequence>
                    <xs:element name="hotel" type="tns:HotelDetail" minOccurs="0" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="RentalAgencies">
                <xs:sequence>
                    <xs:element name="rentalAgency" minOccurs="0" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="name" type="xs:string"/>
                                <xs:element name="address" type="tns:Address"/>
                                <xs:element name="comment" type="xs:string"/>
                                <xs:element name="carOptions" type="tns:CarOptions"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="ReturningFlights">
                <xs:sequence>
                    <xs:element name="flightPlan" type="tns:FlightPlan" minOccurs="0" maxOccurs="unbounded"/>
                    <xs:element name="origin" type="xs:string"/>
                    <xs:element name="destination" type="xs:string"/>
                    <xs:element name="date" type="xs:date"/>
                </xs:sequence>
            </xs:complexType>
            <xs:complexType name="RoomOptions">
                <xs:sequence>
                    <xs:element name="room" maxOccurs="unbounded">
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="roomType" type="xs:string"/>
                                <xs:element name="avgCostPerNight" type="xs:string"/>
                                <xs:element name="roomAmenities" type="xs:string"/>
                            </xs:sequence>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:complexType>

     	    <xs:complexType name="AllSearch">
                <xs:sequence>
                    <xs:element name="searchAirline"  type="tns:AirlineSearch"/>
                    <xs:element name="searchCar"  type="tns:CarSearch"/>
                    <xs:element name="searchHotel"  type="tns:HotelSearch"/>
                </xs:sequence>
            </xs:complexType>
	    <xs:complexType name="AllSearchResults">
                <xs:sequence>
                    <xs:element name="searchAirlineResponse"  type="tns:AirlineSearchResults"/>
                    <xs:element name="searchCarResponse"  type="tns:CarSearchResults"/>
                    <xs:element name="searchHotelResponse"  type="tns:HotelSearchResults"/>
                </xs:sequence>
            </xs:complexType>            
	    <xs:element name="searchAll" type="tns:AllSearch"/>
            <xs:element name="searchAllResponse" type="tns:AllSearchResults"/>
            <xs:element name="searchAll_fault">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="searchAll_fault"  type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>                                 
</xs:schema>	  

   
 
