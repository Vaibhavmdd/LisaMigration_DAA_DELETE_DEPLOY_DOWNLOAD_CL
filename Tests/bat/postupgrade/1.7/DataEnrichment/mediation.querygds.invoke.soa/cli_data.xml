	<ResourceTemplate xsi:type="amxdata:HttpConnectorResourceTemplate" name="httpConnectorRT" host="localhost" port="8777" headerBufferSize="512" requestBufferSize="512"	responseBufferSize="512" maxIdleTime="200000" lowResourceMaxIdleTime="512" description="This is HttpConnector RT for Samples"/>
	<DAA
		xsi:type="amxdata:DAA"
		description="Data enrichment mediation DAA"
		location="Deployment Artifacts/mediation.querygds.invoke.daa"
	        importResourceTemplates="true">
	</DAA>

