<?xml version="1.0" encoding="UTF-8"?>

<amxdata_base:Enterprise
	xmlns:amxdata="http://tibco.com/amxadministrator/command/line/types"
	xmlns:amxdata_base="http://tibco.com/amxadministrator/command/line/types_base"
	xmlns:amxdata_reference="http://tibco.com/amxadministrator/command/line/types_reference"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://tibco.com/amxadministrator/command/line/types_base ../schemas/amxdata_base.xsd http://tibco.com/amxadministrator/command/line/types ../schemas/amxdata.xsd">
	
	<Environment xsi:type="amxdata:Environment" name="DevEnvironment" >		
		<Node xsi:type="amxdata:Node" name="DevNode">

        <ResourceInstance xsi:type="amxdata:ResourceInstance" name="http_client_1" resourceTemplateName="http_client_1"/>
		
		<ResourceInstance
                xsi:type="amxdata:ResourceInstance"
                name="httpConnector1"
                resourceTemplateName="httpConnector1"
                />
				
            <ResourceInstance
                xsi:type="amxdata:ResourceInstance"
                name="http_client_2"
                resourceTemplateName="http_client_2"
                />

		</Node>
		
		<Application xsi:type="amxdata:Application" name="DeclaredFault_Provider">
		<Node name="DevNode"  environmentName="DevEnvironment" />
		<ApplicationTemplate xsi:type="amxdata_reference:ApplicationTemplate_reference" name="DeclaredFault_Provider"/>
		 
		</Application>
		
		<Application xsi:type="amxdata:Application" name="DeclaredFault_Consumer">
		<Node name="DevNode"  environmentName="DevEnvironment" />
		<ApplicationTemplate xsi:type="amxdata_reference:ApplicationTemplate_reference" name="DeclaredFault_Consumer"/>		 
		</Application>
	</Environment>
	
	<ResourceTemplate
        xsi:type="amxdata:HttpClientResourceTemplate"
        name="http_client_2"
        isAcceptRedirect="true"
        connectionTimeout="8888"
        host="localhost"
        port="9095"
        description="This is HttpClient RT for BW"
        >
    </ResourceTemplate>
	
	<ResourceTemplate
        xsi:type="amxdata:HttpClientResourceTemplate"
        name="http_client_1"
        isAcceptRedirect="true"
        connectionTimeout="8888"
        host="localhost"
        port="9095"
        description="This is HttpClient RT for AMX"
       >
			
    </ResourceTemplate>

    <ResourceTemplate
        xsi:type="amxdata:HttpConnectorResourceTemplate"
        name="httpConnector1"
        host="localhost"
        port="9095"
        headerBufferSize="4096"
        requestBufferSize="8192"
        responseBufferSize="24576"
        acceptQueueSize="20"
        lingerTime="1000"
        maxIdleTime="200000"
        lowResourceMaxIdleTime="-1"
        description="This is HttpConnector RT for Provider">	
    </ResourceTemplate>
	
	<DAA xsi:type="amxdata:DAA" description="My DAA for BAT5.1" location="C:/tibco/amx3-v65/administrator/3.2/samples/DeclaredFault_Provider.daa" />
	<DAA xsi:type="amxdata:DAA" description="My DAA for BAT5.1" location="C:/tibco/amx3-v65/administrator/3.2/samples/DeclaredFault_Consumer.daa" />
	<AppTemplate xsi:type="amxdata_base:AppTemplateID" name="DeclaredFault_Provider" version="1.0.0"/>
	<AppTemplate xsi:type="amxdata_base:AppTemplateID" name="DeclaredFault_Consumer" version="1.0.0"/>

</amxdata_base:Enterprise>