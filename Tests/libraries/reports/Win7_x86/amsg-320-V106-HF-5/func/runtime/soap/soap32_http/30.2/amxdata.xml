<?xml version="1.0" encoding="UTF-8"?>

<amxdata_base:Enterprise 
	xmlns:amxdata="http://tibco.com/amxadministrator/command/line/types" 
	xmlns:amxdata_base="http://tibco.com/amxadministrator/command/line/types_base" 
	xmlns:amxdata_reference="http://tibco.com/amxadministrator/command/line/types_reference" 
	xmlns:amxdata_jmsssr="http://tibco.com/amxadministrator/command/line/types_jmssr" 
	xmlns:amxdata_jmssr_base="http://tibco.com/amxadministrator/command/line/types_jmssr_base"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://tibco.com/amxadministrator/command/line/types ../schemas/amxdata.xsd
	http://tibco.com/amxadministrator/command/line/types_base ../schemas/amxdata_base.xsd 
	http://tibco.com/amxadministrator/command/line/types_reference ../schemas/amxdata_reference.xsd 
	http://tibco.com/amxadministrator/command/line/types_jmssr ../schemas/jmssr.xsd 
	http://tibco.com/amxadministrator/command/line/types_jmssr_base ../schemas/jmssr_base.xsd">

    <Environment xsi:type="amxdata:Environment" 
		name="QAEnvironment_jpandya-dt" 
		description="test environment" 
		contact="TIBCO">
		
		<Node xsi:type="amxdata:Node"
			name="QANode1">						

		<ResourceInstance xsi:type="amxdata:ResourceInstance" name="httpConnector" resourceTemplateName="httpConnector"/>		
		</Node>
		
		<Application xsi:type="amxdata:Application" 
			name="it.lifecycleTesting.daa" 
			contact="TIBCO Inc." 
			description="IT-App" importResourceTemplates="false">
			<Component xsi:type="amxdata:Component" name="implementation.lifecycleTesting">
			    <Node name="QANode1" environmentName="QAEnvironment_jpandya-dt"/>
			</Component>	
			<Property xsi:type="amxdata:Property" name="isUndeployUserControlled" value="false"/>
			<Property xsi:type="amxdata:Property" name="throwExceptionAt" value="1"/>
        	<ApplicationTemplate xsi:type="amxdata_reference:ApplicationTemplate_reference"			
				name="it.lifecycleTesting.daa"/>
        </Application>
		
		<Application xsi:type="amxdata:Application" 
			name="testApp" 
			contact="TIBCO Inc." 
			distributionPolicy="PRODUCT_APP"
			description="testApp" importResourceTemplates="false">
			<Component xsi:type="amxdata:Component" name="LifecycleTestingIT1">
			    <Node name="QANode1" environmentName="QAEnvironment_jpandya-dt"/>
			</Component>	
        	<ApplicationTemplate xsi:type="amxdata_reference:ApplicationTemplate_reference"			
				name="testApp"/>	
			<TargetApplication xsi:type="amxdata_reference:Application_reference"
				name="it.lifecycleTesting.daa">
			</TargetApplication>
        </Application>
		
    </Environment>
		<ResourceTemplate xsi:type="amxdata:HttpConnectorResourceTemplate"
		name="httpConnector"
		host="0.0.0.0"
		port="9898"
		headerBufferSize="4096"
		requestBufferSize="8192"
		responseBufferSize="24576"
		maxIdleTime="200000" 
		lowResourceMaxIdleTime="-1"
		description="Http Connector"/>
		
	<DAA xsi:type="amxdata:DAA" 
		description="implementation-lifecycleTesting" 
		location="DAA/implementation-lifecycleTesting.daa"
		importResourceTemplates="false"
		importFeatures="true" />
	<DAA xsi:type="amxdata:DAA" 
		description="implementation-lifecycleTesting" 
		location="DAA/testApp.daa"
		importResourceTemplates="false"
		importFeatures="true" />

</amxdata_base:Enterprise>
