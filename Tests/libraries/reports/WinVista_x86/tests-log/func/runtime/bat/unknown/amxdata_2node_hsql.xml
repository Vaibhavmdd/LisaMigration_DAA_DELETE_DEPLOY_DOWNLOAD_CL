	******************************************************** -->
	
	<AdminConfiguration 
		xsi:type="amxdata_bootstrap:AdminConfiguration" 
		name="amxadmin" 
		initialSuperuser="root" 
		initialSuperuserPassword="t">
		<StatusTransportDetails 
			recoveryTimerTimeout="3000" 
			recoveryAttemptDelay="2000" 
			groupName="amxadmin" 
			userName="admin" 
			password="" 
			serverURL="tcp://jpandya-z400:7222"/>
		<AdminConfigFolders 
			instanceWorkFolder="${TIBCO_AMXADMIN_CONFIG}/amxadmin/private/instanceOne" 
			sharedWorkFolder="${TIBCO_AMXADMIN_CONFIG}/amxadmin/shared" 
			productStagingFolder="${TIBCO_HOME}"/>
		<AppDatabaseDetails 
			jndiName="AdminTeneoResource" 
			name="tibco.admin.appdb" 
			schemaGeneration="update" 
			dialect="org.hibernate.dialect.HSQLDialect">
			<TeneoResource 
				xsi:type="amxdata_bootstrap:TeneoResource">
				<advancedProperties 
					xsi:type="amxdata_bootstrap:Properties">
					<Property xsi:type="amxdata_bootstrap:Property" name="maximumSqlNameLength" value="30"/>
					<Property xsi:type="amxdata_bootstrap:Property" name="sqlCaseStratrgy" value="uppercase"/>
					<Property xsi:type="amxdata_bootstrap:Property" name="currentSessionContextProvider" value="thread"/>
					<Property xsi:type="amxdata_bootstrap:Property" name="emapAsTrueMap" value="false"/>
				</advancedProperties>
			</TeneoResource>
			<InProcessDatabase 
				databaseAlias="amx" 
				dataDirectory="${TIBCO_AMXADMIN_CONFIG}/amxadmin/private/instanceOne/hsqldb/amx"/>
		</AppDatabaseDetails>
		<AuthRealm 
			realmType="database" 
			jaasJndiName="tibco.admin.authrealm.jaas" 
			rmsJndiName="tibco.admin.authrealm.rms">
			<DbRealmDetails>
				<AMXApplicationDB/>
			</DbRealmDetails>
		</AuthRealm>
		<AdminKeyStore 
			adminKeyStoreJNDI="tibco.admin.default.keystore" 
			adminKeyStorePassword="password" 
			keyStoreRefresh="86400000"/>
		<SCMDetails 
			type="filesystem" 
			location="${TIBCO_AMXADMIN_CONFIG}/amxadmin/shared/repo"/>
		<TrinityCredentialServerDetails 
			host="jpandya-z400" 
			port="6041" 
			username="user1" 
			password="password1"/>
		<SystemMessagingBus 
